{
  "name": "mas-scorer",
  "version": "1.0.0",
  "description": "SAS Micro Analytic Score Module Scorer Application",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && npm run zip",
    "build:jobdef": "tsc && cross-env BUILD_MODE=jobdef vite build",
    "zip": "cd dist && bestzip ../dist.zip *",
    "preview": "vite preview",
    "preview:jobdef": "vite preview --outDir dist-jobdef",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@sassoftware/sas-auth-browser": "^0.2.0",
    "axios": "^1.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.12.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-react": "^4.2.0",
    "bestzip": "^2.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.3.0",
    "vite": "^7.3.1",
    "vite-plugin-singlefile": "^2.0.0"
  }
}
